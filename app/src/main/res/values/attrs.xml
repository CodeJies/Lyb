<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LybEditText">
        <attr name="type">
            <enum name="phone" value="0" />
            <enum name="password" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="LybIconTextView">
        <attr name="lybDefaultIcon" format="reference" />
        <attr name="lybHighLightIcon" format="reference" />
        <attr name="direction">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
            <enum name="top" value="2" />
            <enum name="bottom" value="3" />
        </attr>
        <attr name="lybHighLightText" format="reference|color" />
        <attr name="lybDefalutText" format="reference|color" />
    </declare-styleable>
</resources>